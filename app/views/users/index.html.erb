<% provide(:title, "ユーザー一覧ページ") %>
<h1>ユーザー一覧</h1>
<div class="row">
  <div class="col-md-10 col-md-offset-1">
    <table class="table">
      <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= link_to user.name, user_path(user) %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>