<% provide(:title, "出勤中社員一覧") %>

<h1>出勤社員一覧</h1>
<div class="row">
  <div class="col-md-10 col-md-offset-1">
    <table class="table table-hover">
      <thead>
        <tr>
          <td>社員番号</td>
          <td>名前</td>
        </tr>
      </thead>
      <% @working_index.each do |user| %>
        <tbody>
          <tr>
            <td><%= user.employee_number %></td>
            <td><%= user.name %></td>
          </tr>
        </tbody>
      <% end %>
    </table>
  </div>
</div>